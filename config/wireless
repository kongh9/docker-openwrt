config 'wifi-device'    'radio0'
    option 'type'       'mac80211'
    option 'disabled'   '0'
    option 'phy'        "phy0"
    option 'hwmode'     "11g"
    option 'channel'    "1"

config wifi-iface 'wifinet0'
    option device 'radio0'
    option network 'wifi'
    option mode 'ap'
    option ssid 'OpenWrt'
    option key '12345678'
    option encryption 'psk-mixed'
